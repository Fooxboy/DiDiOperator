<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:DiDiOperator.MobileClient"
             xmlns:converters="clr-namespace:DiDiOperator.MobileClient.Converters"
             x:Class="DiDiOperator.MobileClient.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <converters:InversionBoolConverter x:Key="InversionBoolConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
